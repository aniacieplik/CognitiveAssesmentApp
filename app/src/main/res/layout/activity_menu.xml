<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="16dp"
    tools:context=".registration.MenuActivity">

    <Button
        android:id="@+id/tymtest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TYM test"
        android:layout_marginBottom="16dp"/>

    <Button
        android:id="@+id/mistest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="MIS test"
        android:layout_below="@id/tymtest"
        android:layout_marginBottom="16dp"/>

    <Button
        android:id="@+id/bastest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="BAS test"
        android:layout_below="@id/mistest"
        android:layout_marginBottom="16dp"/>

</RelativeLayout>
